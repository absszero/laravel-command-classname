{
    "name": "absszero/laravel-command-classname",
    "description": "Display Laravel command class name from artisan",
    "keywords": [
        "laravel",
        "artisan"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "absszero"
        }
    ],
    "require": {
        "php": ">=5.5",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0",
        "symfony/console": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Absszero\\CommandClassname\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Absszero\\CommandClassnameServiceProvider"
            ]
        }
    }
}
